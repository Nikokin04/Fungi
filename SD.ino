String nameFile;
String nameDirYM, nameDirD;

//Inicializa la lectura de la memoria microSD
void configSD() {
  if (!SD.begin())
  {
    Serial.println("Card failed, or not present");
    SdOk = false;
  }
  else
  {
    Serial.println("SD card OK.");
    SdOk = true;
  }
}

