//Crea el directorio por año y mes dentro de la SD
void createDir_YM() {
  String  nameDir1 = "/";
  nameDir1 += String(timeRTC.YearRTC);
  if (timeRTC.MonthRTC < 10)
  {
    nameDir1 += 0;
  }
  nameDir1 += String(timeRTC.MonthRTC);

  nameDirYM = nameDir1;
  createDir(nameDir1.c_str());
}

//Crea el directorio por día dentro de la SD
void createDir_D() {
  String  nameDir1 = nameDirYM;
  nameDir1 += "/";
  if (timeRTC.DayRTC < 10)
  {
    nameDir1 += 0;
  }
  nameDir1 += String(timeRTC.DayRTC);

  nameDirD = nameDir1;
  
  createDir(nameDir1.c_str());
}

//Crea directorios
void createDir(const char * path) {
  Serial.printf("Creating Dir: %s\n", path);
  if (SD.mkdir(path)) {
    Serial.println("Dir created");
    SdOk = true;
  } else {
    Serial.println("mkdir failed");
    SdOk = false;
  }
}

//Escribe sobre algún archivo de texto dentro de la microSD
void writeFile( const char * path, const char * message) {
  Serial.printf("Writing file: %s\n", path);

  File file = SD.open(path, FILE_WRITE);
  if (!file) {
    Serial.println("Failed to open file for writing");
    SdOk = false;
    return;
  }
  if (file.print(message)) {
    Serial.println("File written");
    SdOk = true;
  } else {
    Serial.println("Write failed");
    SdOk = false;
  }
  file.close();
}

//Escribe al final del archivo, la nueva información obtenida
void appendFile( const char * path, const char * message) {
  Serial.printf("Appending to file: %s\n", path);
  File file = SD.open(path, FILE_APPEND);
  
  if (!file) {
    Serial.println("Failed to open file for appending");
    SdOk = false;
    return;
  }
  if (file.print(message)) {
    Serial.println("Message appended");
    SdOk = true;
  } else {
    Serial.println("Append failed");
    SdOk = false;
  }
  file.close();
}

